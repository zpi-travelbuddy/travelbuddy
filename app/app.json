{
  "expo": {
    "name": "travelbuddy",
    "slug": "travelbuddy",
    "version": "1.0.0",
    "owner": "zpi-travelbuddy",
    "orientation": "portrait",
    "icon": "./assets/images/icon-white.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-white.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.zpitravelbuddy.travelbuddy",
      "userInterfaceStyle": "automatic"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-secure-store"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "e79b62a2-a402-4f89-b52b-6b197f7fdd5d"
      }
    }
  }
}
